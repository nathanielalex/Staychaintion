{
  "canisters": {
    "backend": {
      "type": "motoko",
      "main": "backend/Backend.mo",
      "declarations": {
        "node_compatibility": true
      }
    },
    "internet_identity": {
      "type": "pull",
      "id": "rdmx6-jaaaa-aaaaa-aaadq-cai"
    },
    "frontend": {
      "dependencies": ["backend"],
      "type": "assets",
      "source": ["dist/"]
    },
    "chatbot": {
      "type": "motoko",
      "main": "backend/chatbot.mo"
    },
    "Property_backend": {
      "type": "motoko",
      "main": "backend/Storages/Property/Property.mo"
    },
    "User_backend": {
      "type": "motoko",
      "dependencies": ["Property_backend"],
      "main": "backend/Storages/User/User.mo"
    },
    "Chat_backend": {
      "dependencies": ["User_backend"],
      "type": "motoko",
      "main": "backend/Chat_backend/Chat.mo"
    }
  },
  "defaults": {
    "build": {
      "packtool": "mops sources"
    }
  },
  "output_env_file": ".env",
  "version": 2
}
